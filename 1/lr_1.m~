clear;
load carbig;

good_Acceleration = [];
good_Horsepower = [];

good = 0;
for i = 1 : length(Horsepower)
   if and(not(isnan(Horsepower(i))), not(isnan(Weight(i))))
       good_Acceleration = [good_Acceleration, Acceleration(i)];
       good_Horsepower = [good_Horsepower, Horsepower(i)];
       good = good + 1;
   end
end

bad = length(Horsepower) - go;
